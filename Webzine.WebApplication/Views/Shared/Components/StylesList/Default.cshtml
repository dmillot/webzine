@model  List<Style>
<div class="row">
    <div class="col-12 p-0 m-0">
        <h2>A propos</h2>
    </div>
    <p>Retrouvez les pépites sur notre webzine</p>
</div>
<div class="row">
    <h3>Styles</h3>
    <div class="col-12 p-0 m-0">
        <ul>
            @foreach (Style style in Model)
            {
                @*@Html.DisplayFor(s => style.NomStyle);*@
                <li><a asp-controller="style" asp-action="index" asp-route-libelle="@style.Libelle">@style.Libelle</a></li>
            }

        </ul>
    </div>
</div>
